{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [false],
      "max-line-length": [true, 170],
      "ordered-imports": [
  	true,
  	{
    "import-sources-order": "any",
    "named-imports-order": "any"
  	}
     ],
    "triple-equals": [true, "allow-undefined-check"],
    "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}